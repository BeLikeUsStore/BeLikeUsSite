{
  "rewrites": [
    {
      "source": "/site.webmanifest",
      "destination": "/site.webmanifest"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)", 
      "destination": "/" 
    }
  ],
  "headers": [
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}